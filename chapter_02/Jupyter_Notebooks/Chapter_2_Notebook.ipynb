        "\n",
        "# ---- Parameters ----\n",
        "N_SAMPLES = 8\n",
        "CONFIGS = [\"multiple_choice\", \"generation\"]\n",
        "SPLIT_PREFERENCE = [\"validation\", \"test\", \"train\"]\n",
